<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="240.0" prefWidth="320.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="dev.phil.poobdatv7.controller.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextField fx:id="tfNome" promptText="Insira o nome" />
      <PasswordField fx:id="pfSenha" promptText="Insira a senha" />
      <Button fx:id="btnEntrar" mnemonicParsing="false" onAction="#btnEntrarOnAction" prefHeight="26.0" prefWidth="120.0" text="Entrar" />
   </children>
</VBox>
